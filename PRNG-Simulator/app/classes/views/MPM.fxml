<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" prefHeight="130.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simulation.prng.controllers.algorithms.MPMController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="15.0" prefWidth="580.0" spacing="5.0">
         <children>
            <Label text="Seed 1" textFill="#00000080">
               <font>
                  <Font name="Arial Rounded MT Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="seed1MPMTextField" prefHeight="25.0" prefWidth="535.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="9.0" prefWidth="580.0" spacing="5.0">
         <children>
            <Label text="Seed 2" textFill="#00000080">
               <font>
                  <Font name="Arial Rounded MT Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="seed2MPMTextField" prefHeight="25.0" prefWidth="535.0" />
         </children>
      </HBox>
   </children>
</VBox>
